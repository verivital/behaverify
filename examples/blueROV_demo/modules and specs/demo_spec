LTLSPEC G(((blackboard.variables[variable_names.battery_low_warning] = 1) & (blackboard.variable_exists[variable_names.battery_low_warning])) -> F (active_node = node_names.surface_task));
--proves that if low warning, then surface (eventually)

LTLSPEC G((active_node = -1) -> (((blackboard.variables[variable_names.battery_low_warning] = 1) <-> (blackboard.variables[variable_names.battery_level] < 1)) | !(blackboard.variable_exists[variable_names.battery_low_warning]) | !(blackboard.variable_exists[variable_names.battery_level])));
--proves that battery_low_warning is tied to battery_level

