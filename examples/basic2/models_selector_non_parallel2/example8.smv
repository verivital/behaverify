				(previous_node < 3) & (advanced_resume_non_parallel_1 in {3}) : advanced_resume_non_parallel_1;
				(previous_node < 3) : 3;
				(previous_node < 4) & (advanced_resume_non_parallel_1 in {4}) : advanced_resume_non_parallel_1;
