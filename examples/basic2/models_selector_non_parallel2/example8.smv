				TRUE : active_node;
			esac;
		next(resume_node) :=
			case
				(previous_node < 0) : resume_node;
				!(previous_status = running) : 1;
				(previous_node in{1, 2}) : previous_node;
				TRUE : resume_node;
			esac;
		next(previous_status) :=
