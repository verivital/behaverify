MODULE main
    DEFINE
	ind_0 := 0;
	ind_1 := 1;
	ind_2 :=
	    case
		(ind_0 + ind_1) = 0 : ind_0;
		(ind_0 + ind_1) = 1 : ind_1;
		--(ind_0 + ind_1) = 2 : ind_2;
		TRUE : ind_3;
	    esac;
	ind_3 := 
	    case
		(ind_0 + ind_1) = 0 : ind_0;
		(ind_0 + ind_1) = 1 : ind_1;
		(ind_0 + ind_1) = 2 : ind_2;
		TRUE : ind_1;
	    esac;
    VAR
	a : 0..10;
    ASSIGN
	init(a) := ind_0;
	next(a) := ind_2;