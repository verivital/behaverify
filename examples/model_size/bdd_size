  python3 behaverify.py btree_creator.py create_root --root_args $i 5 True --string_args './size_models/binary_parallel_synch_mix1_selector/models' --min_val 0 --max_val 1 --output_file ./size_models/binary_parallel_synch_mix1_selector/$i.smv --overwrite
