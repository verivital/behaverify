  echo $i
done

python3 process_stats.py ./size_models/binary_parallel_unsynch_mix2_selector

rm ./size_models/binary_parallel_synch_mix2_sequence/stats

for i in {1..4}
do
  python3 behaverify.py btree_creator.py create_root --root_args $i 10 True --string_args './size_models/binary_parallel_synch_mix2_sequence/models' --min_val 0 --max_val 1 --output_file ./size_models/binary_parallel_synch_mix2_sequence/$i.smv --overwrite
  ~/nuXmv -source command_list ./size_models/binary_parallel_synch_mix2_sequence/$i.smv >> ./size_models/binary_parallel_synch_mix2_sequence/stats
  echo $i
done

python3 process_stats.py ./size_models/binary_parallel_synch_mix2_sequence


rm ./size_models/binary_parallel_unsynch_mix2_sequence/stats

for i in {1..4}
do
  python3 behaverify.py btree_creator.py create_root --root_args $i 11 True --string_args './size_models/binary_parallel_unsynch_mix2_sequence/models' --min_val 0 --max_val 1 --output_file ./size_models/binary_parallel_unsynch_mix2_sequence/$i.smv --overwrite
  ~/nuXmv -source command_list ./size_models/binary_parallel_unsynch_mix2_sequence/$i.smv >> ./size_models/binary_parallel_unsynch_mix2_sequence/stats
  echo $i
done

python3 process_stats.py ./size_models/binary_parallel_unsynch_mix2_sequence
