		descent_from_3 := 3;
		descent_from_4 := 4;
		descent_from_5 := 
			case
				(active_node <6) & (resume_relevant_child_6) : descent_from_6;
				(active_node <7) & (resume_relevant_child_7) : descent_from_7;
				(active_node <6) : descent_from_6;
				(active_node <7) : descent_from_7;
