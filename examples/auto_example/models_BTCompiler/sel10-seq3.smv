		sel9 : bt_fallback(sel7, sel6);
		sel7 : bt_fallback(seq3, sel3);
		seq3 : bt_sequence_with_memory(leaf2, leaf3);
		leaf2 : bt_skill;
		leaf3 : bt_skill;
		sel3 : bt_fallback(leaf4, leaf5);
		leaf4 : bt_skill;
		leaf5 : bt_skill;
		sel6 : bt_fallback(seq2, sel2);
		seq2 : bt_sequence_with_memory(leaf6, leaf7);
		leaf6 : bt_skill;
		leaf7 : bt_skill;
		sel2 : bt_fallback(leaf8, leaf9);
		leaf8 : bt_skill;
		leaf9 : bt_skill;
		sel8 : bt_fallback(sel5, sel4);
