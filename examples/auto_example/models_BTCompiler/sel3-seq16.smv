MODULE main
	VAR
		root : bt_sequence_with_memory(seq3, seq37);
		seq3 : bt_sequence_with_memory(seq31, seq34);
		seq31 : bt_sequence_with_memory(seq32, seq33);
		seq32 : bt_sequence_with_memory(seq2, sel2);
		seq2 : bt_sequence_with_memory(leaf12, leaf13);
		leaf12 : bt_skill;
		leaf13 : bt_skill;
		sel2 : bt_fallback(leaf14, leaf15);
		leaf14 : bt_skill;
		leaf15 : bt_skill;
		seq33 : bt_sequence_with_memory(seq1, sel1);
		seq1 : bt_sequence_with_memory(leaf16, leaf17);
		leaf16 : bt_skill;
		leaf17 : bt_skill;
		sel1 : bt_fallback(leaf18, leaf19);
		leaf18 : bt_skill;
		leaf19 : bt_skill;
		seq34 : bt_sequence_with_memory(seq35, seq36);
		seq35 : bt_sequence_with_memory(leaf0, leaf1);
		leaf0 : bt_skill;
		leaf1 : bt_skill;
		seq36 : bt_sequence_with_memory(leaf2, leaf3);
		leaf2 : bt_skill;
		leaf3 : bt_skill;
		seq37 : bt_sequence_with_memory(seq38, seq311);
		seq38 : bt_sequence_with_memory(seq39, seq310);
		seq39 : bt_sequence_with_memory(leaf4, leaf5);
		leaf4 : bt_skill;
		leaf5 : bt_skill;
		seq310 : bt_sequence_with_memory(leaf6, leaf7);
		leaf6 : bt_skill;
		leaf7 : bt_skill;
		seq311 : bt_sequence_with_memory(seq312, sel3);
		seq312 : bt_sequence_with_memory(leaf8, leaf9);
		leaf8 : bt_skill;
		leaf9 : bt_skill;
		sel3 : bt_fallback(leaf10, leaf11);
		leaf10 : bt_skill;
		leaf11 : bt_skill;
