		statuses := [sel_root.status, sel3.status, sel1.status, leaf3.status, leaf4.status, leaf0.status, sel2.status, leaf1.status, leaf2.status];
		statuses_0 := [invalid, sel3.status, sel1.status, leaf3.status, leaf4.status, leaf0.status, sel2.status, leaf1.status, leaf2.status];
		statuses_1 := [invalid, invalid, sel1.status, leaf3.status, leaf4.status, leaf0.status, sel2.status, leaf1.status, leaf2.status];
