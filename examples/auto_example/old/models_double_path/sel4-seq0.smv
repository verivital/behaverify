		statuses_7 := [invalid, invalid, invalid, invalid, invalid, invalid, invalid, invalid, leaf2.status];
		statuses_8 := [invalid, invalid, invalid, invalid, invalid, invalid, invalid, invalid, invalid];
		resume_from_node_0 := -3;
