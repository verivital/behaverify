    }#
    variable{bl foo array 3 VAR [0, 10] range assign{result{'serene_index'}}}
    variable{bl trom array 2 DEFINE INT per_index assign{result{(addition, (index, foo, 0), (index, foo, 1), (index, foo, 2))}}assign{result{(multiplication, (index, foo, 0), (index, foo, 1), (index, foo, 2))}}}
    variable{bl index_var VAR [0, 2] assign{result{0, 1, 2}}}
    variable{local bar array 3 VAR {'increase', 'decrease', 'nope'} per_index assign{result{'increase'}} assign{result{'increase', 'decrease'}} assign{case{False}result{'increase', 'decrease'}result{'nope'}}}
} end_variables

