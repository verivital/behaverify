MODULE main
    DEFINE
	nah := swconst(2, 5);
	nah2 :=
	    case
		hey = 0 : swconst(0, 5);
		hey = 1 : swconst(1, 5);
		hey = 2 : swconst(2, 5);
		hey = 3 : swconst(3, 5);
		hey = 4 : swconst(4, 5);
		hey = 5 : swconst(5, 5);
		hey = 6 : swconst(6, 5);
		hey = 7 : swconst(7, 5);
		hey = 8 : swconst(8, 5);
	    esac;
	
    VAR
	hey : 0..8;
	meh : boolean;
    ASSIGN
	meh := nah2 > nah;