MODULE main
    VAR
	pos1 : signed word[65];
	neg1 : signed word[65];
	zero1 : signed word[65];
	pos2 : signed word[65];
	neg2 : signed word[65];
	zero2 : signed word[65];
    ASSIGN
	pos1 := swconst(1, 65);
	neg1 := swconst(-1, 65);
	zero1 := pos1 + neg1;
	pos2 := 0sd65_1;
	neg2 := -0sd65_1;
	zero2 := pos2 + neg2;